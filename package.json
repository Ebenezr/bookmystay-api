{
  "name": "ecommerce-api",
  "version": "1.0.0",
  "description": "ecommerce nodejs api",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/index.ts",
    "prisma": "prisma",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "generate:prisma": "npx prisma migrate dev --name init && npm run seed ",
    "seed": "ts-node ./prisma/seed.ts --accept-data-loss"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "author": "Ebenezar",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.34.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "8.33.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-redis-cache": "^1.1.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^2.0.20",
    "redis": "^4.6.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@prisma/client": "^4.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "cross-env": "^7.0.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1"
  }
}
